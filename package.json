{
  "name": "teleproto",
  "version": "1.222.0",
  "description": "NodeJS MTProto API Telegram client library,",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanyok12345/teleproto.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "generate:tl": "ts-node --transpile-only teleproto_generator/generate.ts",
    "publish:dist-manifest": "node -e \"const fs=require('fs');const pkg=require('./package.json');const out={name:pkg.name,version:pkg.version,description:pkg.description,main:pkg.main,types:pkg.types,repository:pkg.repository,license:pkg.license,bugs:pkg.bugs,keywords:pkg.keywords,homepage:pkg.homepage,dependencies:pkg.dependencies};fs.writeFileSync('dist/package.json',JSON.stringify(out,null,2)+'\\n');\"",
    "publish:prepare": "npm run clean && npm run generate:tl && npm run build && npm run typecheck && npm run publish:dist-manifest && cp README.md LICENSE.txt dist/ && cp teleproto/define.d.ts dist/define.d.ts && rm -f dist/tsconfig.tsbuildinfo",
    "publish:check": "npm run publish:prepare && npm pack ./dist --dry-run",
    "publish:latest": "npm run publish:prepare && npm publish ./dist --tag latest",
    "publishOnly": "npm run publish:latest",
    "docs": "typedoc"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanyok12345/teleproto/issues"
  },
  "keywords": [
    "telegram",
    "mtproto",
    "teleproto",
    "client",
    "nodejs",
    "typescript",
    "api"
  ],
  "homepage": "https://github.com/sanyok12345/teleproto#readme",
  "devDependencies": {
    "@types/mime": "^2.0.3",
    "@types/node": "^22.15.18",
    "@types/node-localstorage": "^1.3.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.28.7",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "async-mutex": "^0.3.0",
    "big-integer": "^1.6.48",
    "mime": "^3.0.0",
    "node-localstorage": "^2.2.1",
    "socks": "^2.6.2",
    "store2": "^2.13.0",
    "ts-custom-error": "^3.2.0"
  }
}
